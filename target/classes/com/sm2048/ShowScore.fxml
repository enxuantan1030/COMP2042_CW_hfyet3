<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import java.net.URL?>
<GridPane fx:id="grid" alignment="CENTER" hgap="10.0" vgap="10.0" xmlns:fx="http://javafx.com/fxml" fx:controller="com.sm2048.Accounts.Table">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <Label style="-fx-font: BOLD 30 Monaco;" text="High Score List" GridPane.columnIndex="0" GridPane.rowIndex="0">
    </Label>
    <TableView fx:id="tableView" GridPane.columnIndex="0" GridPane.rowIndex="1">
        <columns>
            <TableColumn text="Name" fx:id="tusername">
                <cellValueFactory><PropertyValueFactory />
                </cellValueFactory>
            </TableColumn>
            <TableColumn text="Score" fx:id="tscore">
                <cellValueFactory><PropertyValueFactory />
                </cellValueFactory>
            </TableColumn>
            <TableColumn text="Time" fx:id="ttime">
                <cellValueFactory><PropertyValueFactory/>
                </cellValueFactory>
            </TableColumn>
        </columns>
    </TableView>

    <stylesheets>
        <URL value="@Style.css" />
    </stylesheets>
</GridPane>
